{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".ts,.js\" --out-dir dist --copy-files ",
    "docker-restart": "docker-compose down && docker-compose up -d && yarn orm migration:run && yarn seed:admin",
    "seed:admin": "ts-node-dev -r tsconfig-paths/register src/shared/infra/database/postgres/typeorm/seeds/admin.ts",
    "docker-up": "docker-compose up -d && yarn orm migration:run && yarn seed:admin",
    "docker-prune": "docker-compose down && docker system prune -a -f --volumes",
    "dev": "ts-node-dev --poll --respawn -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "orm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "set NODE_ENV=test&& jest --runInBand --detectOpenHandles"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "aws-sdk": "^2.1092.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "csv-parse": "^5.0.4",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.42",
    "uuid": "^8.3.2",
    "yarn-upgrade-all": "^0.6.1"
  }
}
